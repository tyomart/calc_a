{ "builds": [
    {
      "src": "src/**/*",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
    {
  "rewrites":  [
    {"source": "/(.*)", "destination": "/"}
  ]
}
}
